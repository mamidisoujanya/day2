#include<stdio.h>
#include<stdlib.h>
int main()
{
int a[100],i,position,value;

printf("enter the array of elements");

for(i=0;i<4;i++)
scanf("%d",&a[i]);

printf("Enter the location to insert an element\n");
   scanf("%d", &position);
 
   printf("Enter the value to insert\n");
   scanf("%d", &value);

for(i=3;i>=position-1;i--)
a[i+1]=a[i];

a[position-1]=value;

for(i=0;i<=4;i++)
printf("%d\n",a[i]);

return 0;

}
