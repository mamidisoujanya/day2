#include<stdio.h>
#include<stdlib.h>
int main()
{
int a[100],i,position,n;
printf("enter the n value");
scanf("%d",&n);

printf("enter the array of elements");

for(i=0;i<n;i++)
scanf("%d",&a[i]);

printf("Enter the location to delete an element\n");
   scanf("%d", &position);
 for(i=position;i<n;i++)
a[i]=a[i+1];
for(i=0;i<n-1;i++)
printf("%d\n",a[i]);

return 0;

}
