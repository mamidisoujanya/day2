scanf("%d",&n);
printf("enter the array\n");
for(i=0;i<n;i++)
scanf("%d",&array[i]);
j=0;
i=n-1;
while(j<i)
{
temp=array[j];
array[j]=array[i];
array[i]=temp;
j++;
i--;
}
printf("reverse the array is\n");
for(j=0;j<n;j++)

printf(" %d\n",array[j]);
