#include<stdio.h>
#include<stdlib.h>
int main()
{
int array[100],b[100];
int i,j,n;
printf("enter the length of array");
scanf("%d",&n);
printf("enter the array\n");
for(i=0;i<n;i++)
scanf("%d",&array[i]);
for(i=n-1,j=0;i>=0;i--,j++)
b[j]=array[i];
printf("reverse the array is\n");
for(j=0;j<n;j++)

printf(" %d\n",b[j]);
return 0;
}
