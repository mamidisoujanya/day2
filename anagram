#include<stdio.h>
#include<stdlib.h>
int main()
{
char str[100],str1[100];
int i,j,same=0,diff=0,l,l1;
printf("enter the string one\n");
scanf("%[^\n]%*c",str);
printf("enter the string two\n");
scanf("%[^\n]%*C",str1);
for(l=0;str[l]!='\0';l++);
for(l1=0;str1[l1]!='\0';l1++);
if(l==l1)
{
l=l1;
for(i=0;i<l;i++)
{
same=0;
for(j=0;j<l;j++)
{
if(str[i]==str1[j])
{
same=1;
}
}
}
if(same==0)
printf("\ngiven strings is not anagram");
else 
printf("\ngiven strings is  a anagram");
}
else
printf("strings lengths are not equal ");
return 0;
}
