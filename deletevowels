#include<stdio.h>
#include<stdlib.h>
int main()
{
char str[50],p;
int i,j,k=0,l;
printf("enter the string\n");
scanf("%s",str);

for(i=0;str[i]!='\0';i++)
{
while((str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U'))
{
for(j=i;str[j]!='\0';j++){

str[j]=str[j+1];

}
}
}
printf("output %s",str);
return 0;
}
