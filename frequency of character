#include<stdio.h>
#include<stdlib.h>
int main()
{
char str[100],ch;
int i,count=0;
printf("enter the string");
scanf("%[^\n]%*c",str);
printf("enter the character");
scanf("%c",&ch);
for(i=0;str[i]!='\0';i++)
{
if(ch==str[i])

count++;
}
printf("the frequency of character is %d",count);
return 0;
}
